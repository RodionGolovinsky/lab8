<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="378.0" prefWidth="558.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="nameFld" layoutX="167.0" layoutY="59.0" prefHeight="30.0" prefWidth="224.0" promptText="username" />
      <PasswordField fx:id="passwordFld" layoutX="167.0" layoutY="158.0" prefHeight="30.0" prefWidth="224.0" promptText="password" />
      <CheckBox fx:id="registerCheck" layoutX="167.0" layoutY="218.0" mnemonicParsing="false" prefHeight="11.0" prefWidth="155.0" text="register">
         <font>
            <Font size="13.0" />
         </font>
      </CheckBox>
      <Button fx:id="signInBut" layoutX="253.0" layoutY="281.0" mnemonicParsing="false" onAction="#execute" text="sign in">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Label fx:id="resLbl" alignment="TOP_CENTER" layoutX="167.0" layoutY="98.0" prefHeight="50.0" prefWidth="224.0" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
