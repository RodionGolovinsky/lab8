CREATE TABLE persons_table (
id SERIAL PRIMARY KEY,
name TEXT,
coordinates_x NUMERIC,
coordinates_y INT,
height INT,
creation_date TEXT,
eye_color TEXT,
hair_color TEXT,
nationality TEXT,
location_x NUMERIC,
location_y NUMERIC,
location_z BIGINT,
location_name TEXT,
user_id BIGINT
);

CREATE TABLE my_users (
id BIGINT,
username TEXT,
password TEXT
);


